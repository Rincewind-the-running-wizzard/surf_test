stage('git push') {
    steps {
        withCredentials([
            gitUsernamePassword(credentialsId: 'github-token1', gitToolName: 'Default')
        ]) {
            sh '''
                 touch job.log
                 echo `date +%Y-%m-%d:%H:%M:%S` - PRjob executed >> job.log
                 git add .
                 git commit -m "Added log"
                 git push
            '''
        }
    }
}
